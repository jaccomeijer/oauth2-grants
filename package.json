{
  "name": "@jaccomeijer/oauth2-grants",
  "version": "6.31.1",
  "description": "Oauth 2.0 server",
  "keywords": [
    "authentication",
    "express",
    "oauth",
    "oauth2",
    "openid",
    "security",
    "typescript"
  ],
  "homepage": "https://github.com/jaccomeijer/oauth2-grants#readme",
  "bugs": {
    "url": "https://github.com/jaccomeijer/oauth2-grants/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/oauth2-grants.git"
  },
  "license": "MIT",
  "author": "Jacco Meijer (https://www.jaccomeijer.nl)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@jaccomeijer.nl",
      "url": "https://www.jaccomeijer.nl"
    }
  ],
  "exports": {
    "./plain": {
      "import": "./build/plain.mjs",
      "require": "./build/plain.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  },
  "files": [
    "CHANGELOG.md",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:compile": "rollup --config",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc -p tsconfig.json --declaration --emitDeclarationOnly --declarationDir build",
    "clean": "rimraf build/*",
    "dev": "rollup --config --watch"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "5.0.0-alpha.8",
    "jsonwebtoken": "^8.5.1",
    "jsrsasign": "10.4.1",
    "ms": "^2.1.3",
    "uri-js": "^4.4.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-modules-commonjs": "7.16.0",
    "@babel/plugin-transform-modules-umd": "7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@emotion/babel-plugin": "11.3.0",
    "@jaccomeijer/make": "6.30.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/jsrsasign": "8.0.13",
    "@types/node": "16.11.7",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "babel-loader": "8.2.3",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.59.0",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.0.7",
    "tslib": "2.3.1",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.11.1"
  }
}